{
  "expo": {
    "name": "GrupaBuk",
    "slug": "GrupaBuk",
    "version": "1.3.0",
    "orientation": "default",
    "icon": "./assets/logo.png",
    "entryPoint": "index.ts",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.konnar55.GrupaBukk"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.konnar55.GrupaBuk",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "androidNavigationBar": {
      "visible": "immersive",
      "backgroundColor": "#00000000",
      "barStyle": "light",
      "behavior": "overlay-swipe"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-asset",
      "expo-web-browser",
      [
        "expo-navigation-bar",
        {
          "visible": "immersive",
          "backgroundColor": "#00000000",
          "barStyle": "light",
          "behavior": "overlay-swipe"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c5167f81-69b1-4ce6-a427-f4a5cbbd3cc9"
      },
      "expo-navigation-bar": {
        "visible": "immersive",
        "backgroundColor": "#00000000",
        "barStyle": "light",
        "behavior": "overlay-swipe"
      }
    },
    "owner": "konnar55"
  }
}
